@base <http://graphity.org/ontology/> .

@prefix : <> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix dct: <http://purl.org/dc/terms/> .
@prefix sioc: <http://rdfs.org/sioc/ns#> .
@prefix sp: <http://spinrdf.org/sp#> .
@prefix void: <http://rdfs.org/ns/void#> .
@prefix sd: <http://www.w3.org/ns/sparql-service-description#> .
@prefix vann: <http://purl.org/vocab/vann/> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix geo: <http://www.w3.org/2003/01/geo/wgs84_pos#> .

: a owl:Ontology ;
    dct:title "Graphity ontology" ;
    rdfs:label "Graphity ontology" ;
    dct:created "2012-02-09T20:54:00+01:00"^^xsd:dateTime ;
    dct:modified "2012-10-25T13:42:00+03:00"^^xsd:dateTime ;
    vann:preferredNamespaceUri "http://graphity.org/ontology/" ;
    vann:preferredNamespacePrefix "g";
    foaf:homepage : ;
    dct:license <http://www.opendatacommons.org/licenses/pddl/1.0/> ;
    dct:creator <http://semantic-web.dk#martynas> .

:service a owl:ObjectProperty ;
    rdfs:label "uses SPARQL service" ;
    rdfs:isDefinedBy : ;
    rdfs:range sd:Service .

:mode a owl:ObjectProperty ;
    rdfs:label "uses XSLT mode" ;
    rdfs:isDefinedBy : ;
    rdfs:range :XSLTMode .

:apiKey a owl:DatatypeProperty ;
    rdfs:label "SPARQL endpoint authentication key" ;
    rdfs:domain sd:Service ;
    rdfs:range xsd:string .

# XSLT MODES

:XSLTMode a owl:Class ;
    rdfs:label "XSLT template mode" .

:XSLTPageMode a owl:Class ;
    rdfs:subClassOf :XSLTMode ;
    rdfs:label "XSLT page template mode" .

:InputMode a :XSLTMode ;
    dct:title "Edit" ;
    rdfs:isDefinedBy : ;
    void:inDataset _:dataset .
:ListMode a :XSLTMode, :XSLTPageMode ;
    dct:title "List" ;
    rdfs:isDefinedBy : ;
    void:inDataset _:dataset .
:TableMode a :XSLTMode, :XSLTPageMode ;
    dct:title "Table" ;
    rdfs:isDefinedBy : ;
    void:inDataset _:dataset .